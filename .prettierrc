{
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "semi": false,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/components/(.*)$",
        "^@/hooks/(.*)$",
        "^@/shared/(.*)$",
        "^@/services/(.*)$",
        "^@/assets/(.*)$",
        "^@/utils/(.*)$",
        "^@/config/(.*)$",
        "^@/providers/(.*)$",
        "^@/components/(.*)$",
        "^@/components/(.*)$",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}